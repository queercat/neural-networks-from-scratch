    "# model out 4x3 output layer\n",
    "\n",
    "import numpy as np\n",
    "\n",
    "inputs = np.array([1, 2, 3, 2.5])\n",
    "weights = np.array([[0.2, 0.8, -0.5, 1.0], [0.5, -0.91, 0.26, -0.5], [-0.26, -0.27, 0.17, 0.87]])\n",
    "bias = np.array([2, 3, 0.5])\n",
    "\n",
    "output = (inputs * weights).sum(axis=1) + bias\n",
    "\n",
    "print(output)\n"
   ]
  }
